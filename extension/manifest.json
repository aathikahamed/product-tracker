{
  "manifest_version": 2,
  "name": "Highlight",
  "version": "1.0.0",
  "description": "Google Chrome",
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs"],
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.6.0/Chart.min.js; object-src 'self'"
}
